@echo off
D:
del /q NMA.COM > NUL
del /q NMA.EXE > NUL
echo Trying to compile COM
CALL E:\TESTDATA\COM.BAT NMA
echo Trying to compile EXE
CALL E:\TESTDATA\EXE.BAT NMA

E:
CD E:\TESTS
TASM cmp.asm > NUL
TLINK cmp > NUL
CD E:\TESTDATA
del /q NMA.COM > NUL
del /q NMA.EXE > NUL

COPY ..\TESTS\CMP.EXE . > NUL
COPY D:\NMA.COM . > NUL
COPY D:\NMA.EXE . > NUL
DEL D:\RESULT.TXT > NUL

echo INPUT01
CALL AUTOONE INPUT01
echo INPUT02
CALL AUTOONE INPUT02
echo INPUT03
CALL AUTOONE INPUT03
echo EDGE01
CALL AUTOONE EDGE01
echo EDGE02
CALL AUTOONE EDGE02

:end
del NMA.COM
del NMA.EXE
del CMP.EXE
CD D:\
type D:\RESULT.TXT

exit